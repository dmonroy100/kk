<%- include('bootheader') -%>
<%- include('menubar') -%>
    <div class="jumbotron">
        <h1>Kollege Kitchen Profile <%= user.userName %></h1>
        <img src="<%= user.profilePicURL%>" width="200">
        <hr>
        <p> Welcome <%= user.googlename %>
            with email <%= user.googleemail %>
            You are logged in to Kollege Kitchen.
        </p>
        <p> Last updated on <%= user.lastUpdate %> </p>
        <hr>
        userName: <input type="text" name = "userName" value="<%= user.userName %>"><br>
        profilePicURL: <input type = "text" name="profilePicURL"
                                        value="<%= user.profilePicURL %>"><br>
        <input type="submit" class="btn btn-lg btn-primary">
    </div>

<%- include('bootfooter') -%>
